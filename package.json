{
  "name": "guidesmiths-hackathon",
  "version": "0.0.1",
  "description": "React application for the 2018 GuideSmiths Hackathon",
  "scripts": {
    "start": "SERVICE_ENV=production node index.js",
    "wait-on": "./node_modules/.bin/wait-on",
    "local": "SERVICE_ENV=local webpack --open --mode development --inline --hot --config webpack.dev.js",
    "dev": "SERVICE_ENV=development webpack-dev-server --open --mode development --inline --hot --config webpack.dev.js",
    "server": "node index.js",
    "analyze": "webpack --config webpack.analyze.js --mode production",
    "build-dev": "webpack --config webpack.dev.js --mode development",
    "build": "webpack --config webpack.prod.js --mode production"
  },
  "dependencies": {
    "optional": "^0.1.4",
    "bunyan": "^1.8.12",
    "chalk": "^2.4.1",
    "concurrently": "^4.0.1",
    "confabulous": "^1.5.5",
    "connected-react-router": "^4.3.0",
    "helmet": "^3.13.0",
    "history": "^4.7.2",
    "hogan.js": "^3.0.2",
    "isomorphic-unfetch": "^2.1.1",
    "loaders.css": "^0.1.2",
    "on-headers": "^1.0.1",
    "prepper": "^1.1.0",
    "prop-types": "^15.6.0",
    "ramda": "^0.25.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^4.3.3",
    "react-loaders": "^3.0.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "styled-components": "^3.3.3",
    "systemic": "^3.0.0",
    "systemic-domain-runner": "^1.1.0",
    "systemic-express": "^1.1.1",
    "uglifyjs-webpack-plugin": "^1.2.6",
    "webpack": "^4.12.0",
    "webpack-merge": "^4.1.3",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|jpg|png)$": "<rootDir>/test/setupTests.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/test/setupTests.js",
    "testURL": "http://localhost/",
    "testMatch": [
      "**/__tests__/**/*.js?(x)",
      "**/(src|test)/**/?(*.)+(spec|test).js?(x)"
    ]
  },
  "devDependencies": {
    "@types/classnames": "^2.2.4",
    "babel-core": "^6.26.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "compression-webpack-plugin": "^1.1.12",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.4",
    "node-sass": "^4.5.3",
    "puppeteer": "^1.5.0",
    "react-test-renderer": "^16.4.2",
    "redux-mock-store": "^1.5.3",
    "redux-saga-test-plan": "^3.7.0",
    "sass-loader": "^6.0.6",
    "selenium-standalone": "^6.15.2",
    "style-loader": "^0.18.2",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  }
}
